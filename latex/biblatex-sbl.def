      test {\ifcsstring{blx@shorthand}{true}}
      or
      (
        test {\ifcsstring{blx@shorthand}{short}}
        and
        test {\ifciteseen}
      )
      or
      (
        test {\ifcsstring{blx@shorthand}{intro}}
        and
        test {\ifciteseen}
      )
    )
  }
    {#1}
    {#2}}

