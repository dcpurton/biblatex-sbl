    {#2}}

\newbibmacro*{ifneedstitlein}[2]{%
  \usebibmacro{ifbooktitleormaintitle}
    {\ifboolexpr{
       togl {inentrytype}
       or
       not test {\usebibmacro{ifusevolumeandpart}}
    }
      {#1}
      {#2}}
    {#2}}

\newbibmacro{pagesin}{%
  \usebibmacro{ifbooktitleormaintitle}
    {\iffieldundef{pages}
